{
  "all": true,
  "include": ["src/**/*.{js,jsx,ts,tsx}"],
  "exclude": [
    "src/**/*.test.{js,jsx,ts,tsx}",
    "src/**/*.spec.{js,jsx,ts,tsx}",
    "src/**/__tests__/**",
    "src/**/__mocks__/**",
    "e2e/**",
    "**/*.d.ts",
    "**/*.config.{js,ts}",
    "src/types/**"
  ],
  "reporter": ["html", "json", "text", "lcov"],
  "report-dir": "coverage-e2e",
  "temp-dir": ".nyc_output",
  "extension": [".js", ".jsx", ".ts", ".tsx"],
  "check-coverage": false,
  "lines": 60,
  "statements": 60,
  "functions": 60,
  "branches": 50
}
